{
  "name": "fe-weekly-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "build:ts": "tsc",
    "start": "nuxt start",
    "nuxt": "cross-env NODE_ENV=development nuxt",
    "generate": "nuxt generate",
    "lint:js": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "server": "nodemon --watch server -e ts,tsx --exec ts-node server/index.ts",
    "start:prod": "cross-env NODE_ENV=production node ./dist/index.js & pm2 start pm2.config.js",
    "dev": "cross-env NODE_ENV=development nuxt & nodemon --watch server -e ts,tsx --exec ts-node server/index.ts"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@koa/router": "^9.4.0",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/dotenv": "^1.4.1",
    "cross-env": "^7.0.2",
    "date-fns": "^2.16.1",
    "koa-static": "^5.0.0",
    "@types/jwt-decode": "^2.2.1",
    "ant-design-vue": "^1.6.4",
    "argon2": "^0.27.0",
    "axios": "^0.20.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.0.0-beta.2",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-jwt": "^4.0.0",
    "mysql": "^2.18.1",
    "nodemailer": "^6.4.11",
    "nuxt": "^2.14.0",
    "qs": "^6.9.4",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "typeorm": "^0.2.26"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/koa": "^2.11.4",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa__cors": "^3.0.1",
    "@types/koa__router": "^8.0.2",
    "@types/node": "^14.10.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
